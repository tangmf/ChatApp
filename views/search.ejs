          <a class="nav-link" href="/about">About</a>
        </li>

      </ul>

    </div>
  </nav>
  <h2 class='page-title'>Rooms Available</h2>
  <div id="room-container">
    
    <% Object.keys(rooms).forEach(room=> { %>
      <div>
        <%= room %>: <a href="/<%= room %>">Join</a>
      </div>

      <% }) %>
  </div>
  <a href="/createroom">Create Room</a>
